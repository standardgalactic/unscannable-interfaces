<span class="hljs-keyword">def</span> <span class="hljs-title function_">increment</span>(<span class="hljs-params">x</span>):
    <span class="hljs-keyword">return</span> x + <span class="hljs-number">1</span>

<span class="hljs-keyword">def</span> <span class="hljs-title function_">double_and_increment</span>(<span class="hljs-params">x</span>):
