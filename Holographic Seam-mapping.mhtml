    ax.set_ylim(-<span class="hljs-number">1.5</span>, <span class="hljs-number">1.5</span>)
    <span class="hljs-keyword">return</span> ln,

<span class="hljs-keyword">def</span> <span class="hljs-title function_">update</span>(<span class="hljs-params">frame</span>):
    ln.set_data(x[frame], v[frame])
    <span class="hljs-keyword">return</span> ln,

ani = FuncAnimation(fig, update, frames=<span class="hljs-built_in">range</span>(<span class="hljs-built_in">len</span>(t)),
                    init_func=init, blit=<span class="hljs-literal">True</span>)

plt.show()
