<span class="hljs-comment"># Set up the figure and axis</span>
fig, ax = plt.subplots()
cax = ax.matshow(grid, cmap=<span class="hljs-string">'gray'</span>)

<span class="hljs-keyword">def</span> <span class="hljs-title function_">update</span>(<span class="hljs-params">*args</span>):
    <span class="hljs-string">"""
    Update function for animation.
    """</span>
